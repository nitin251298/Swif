      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/SWIFAPPX_LOGO-2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.breezlab.swif",
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "89753a5c-e39f-4145-a4d0-9fa22001be96"
      }
    }
  }
}
