{
  "expo": {
    "name": "Swif",
    "slug": "Mobile",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/SWIFAPPX_LOGO-2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SWIFAPPX_LOGO-Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
